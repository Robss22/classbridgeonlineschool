    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1",
    "recharts": "^2.12.0",
    "resend": "^3.1.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^2.5.4",
    "uuid": "^10.0.0",
    "zod": "^4.0.17"
