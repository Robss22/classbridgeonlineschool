{
  "name": "classbridge-online-school",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:development": "NODE_ENV=development next dev",
    "dev:production": "NODE_ENV=production next dev",
    "build": "next build",
    "build:development": "NODE_ENV=development next build",
    "build:production": "NODE_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:staging": "cross-env NODE_ENV=staging next build",
    "start": "next start",
    "start:development": "NODE_ENV=development next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rimraf .next out dist",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "clean:all": "npm run clean && npm run clean:deps",
    "analyze": "npm run build:analyze && npx @next/bundle-analyzer .next/static/chunks",
    "optimize:images": "npx imagemin public/**/*.{jpg,jpeg,png,svg,webp} --out-dir=public/optimized",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deploy:preview": "npm run build:staging && npm run start:production",
    "deploy:production": "npm run build:production && npm run start:production",
    "fix-errors": "powershell -ExecutionPolicy Bypass -File fix-build-errors.ps1",
    "postinstall": "echo 'Skipping type-check for now'",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed'"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.9.0",
